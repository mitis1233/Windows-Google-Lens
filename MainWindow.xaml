<Window x:Name="MainUIWindow" x:Class="Windows_Google_Lens.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Windows_Google_Lens"
        mc:Ignorable="d"
        Title="Windows Google Lens" Height="250" Width="250" FontSize="16" ResizeMode="NoResize">
    <Grid>
        <Button x:Name="scrennshotSearch" Content="Screenshot search" HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top" Width="169" Height="43" FontWeight="Bold" Click="scrennshotSearch_Click"/>
        <Button x:Name="clipboardSearch" Content="Clipboard search" HorizontalAlignment="Center" Margin="0,58,0,0" VerticalAlignment="Top" Width="169" Height="43" FontWeight="Bold"/>
        <Button x:Name="fileSearch" Content="File search" HorizontalAlignment="Center" Margin="0,151,0,0" VerticalAlignment="Top" Width="169" Height="43" FontWeight="Bold"/>
    </Grid>
</Window>
